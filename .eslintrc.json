{
  "extends": ["eslint:recommended", "next", "next/core-web-vitals", "eslint:recommended"],
  "env": {
    "browser": true,
    "es6": true
  },  
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": 0,
    "no-unused-vars": 0,
    "react/no-unescaped-entities": 0,
    "no-useless-escape": 0
  }
}
